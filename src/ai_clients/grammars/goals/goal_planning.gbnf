root ::= GoalPlan
Step ::= "{"   ws   "\"id\":"   ws   number   ","   ws   "\"name\":"   ws   string   ","   ws   "\"status\":"   ws   string   ","   ws   "\"preceding_steps\":"   ws   numberlist   "}"
Steplist ::= "[]" | "["   ws   Step   (","   ws   Step)*?   "]"
GoalPlan ::= "{"   ws   "\"asynchronous_steps\":"   ws   Steplist   ","   ws   "\"synchronous_steps\":"   ws   Steplist   "}"
GoalPlanlist ::= "[]" | "["   ws   GoalPlan   (","   ws   GoalPlan)*   "]"
string ::= "\""   ([^"]*)   "\""
boolean ::= "true" | "false"
ws ::= [ \t\n]*
number ::= [0-9]+   "."?   [0-9]*
stringlist ::= "["   ws   "]" | "["   ws   string   (","   ws   string)*?   ws   "]"
numberlist ::= "["   ws   "]" | "["   ws   number   (","   ws   number)*?   ws   "]"