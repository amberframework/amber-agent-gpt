root ::= GoalPlan
GoalPlan ::= "{ \"asynchronous_steps\": "  Steplist  ", \"synchronous_steps\": "  Steplist  " }"
Step ::= "{ \"id\": "  number  ",\"name\":"  string  ",\"status\":"  string  ",\"preceding_steps\":"  numberlist  "}"
Steplist ::= "[]" | "["  Step  (","   Step)*?   "]"
string ::= "\""  ([^"]*)  "\""
boolean ::= "true" | "false"
number ::= [0-9]+  "."?  [0-9]*
stringlist ::= "[]" | "["  string  (","   string)*?  "]"
numberlist ::= "[]" | "["  number  (","  number)*?  "]"